.c-left-slide-label-view {
  position: relative;
  width: 100%;
}
.c-left-slide-item-info,
.c-left-slide-item-oper {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.c-left-slide-item-info {
  display: flex;
  z-index: 2;
  background: white;
  transition: left 0.2s ease-in-out;
}
