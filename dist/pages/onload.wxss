page {
  background-color: #ffffff;
}
.onload-img-view {
  width: 100%;
  height: 500rpx;
}
button {
  width: 690rpx;
  margin: 30rpx auto 0px auto;
  height: 96rpx;
  line-height: 96rpx;
  background-color: #1BB51D;
  font-size: 28rpx;
  color: #ffffff;
  position: relative;
  display: flex;
  justify-content: center;
}
.button-img-view {
  width: 40rpx;
  height: 40rpx;
  margin: 10rpx 10rpx;
}
.hover-button {
  background-color: #169018;
}
.modal-mask-dialog {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 300;
  color: #fff;
}
.fixed-view-dialog {
  position: fixed;
  width: 558rpx;
  height: 498rpx;
  left: 96rpx;
  top: 314rpx;
  background-color: #ffffff;
  z-index: 400;
  overflow: hidden;
  border-radius: 20rpx;
}
.dialog-title-view {
  margin: 62rpx auto 0px auto;
  width: 300rpx;
  text-align: center;
  font-size: 36rpx;
  color: #4f5050;
  font-weight: 510;
}
.dialog-text-view {
  width: 440rpx;
  margin: 20rpx auto 0px auto;
  font-size: 26rpx;
  color: #b4b4b4;
}
.dialog-fixed-button {
  margin: 110rpx auto 0px auto;
  width: 420rpx;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  border-radius: 42rpx;
  background-color: #1BB51D;
  color: #ffffff;
  font-size: 32rpx;
}
.dialog-bottom-view {
  text-decoration: underline;
  font-size: 26rpx;
  width: 420rpx;
  height: 66rpx;
  line-height: 66rpx;
  margin: 20rpx auto 0px auto;
  text-align: center;
  color: #9057a8;
}
