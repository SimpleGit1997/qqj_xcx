.map-content-view {
  width: 100%;
  height: 400rpx;
}
.default-label-view {
  margin: 0px 0px 20rpx 0rpx;
  background-color: #ffffff;
  padding: 30rpx 20rpx 20rpx 20rpx;
  box-sizing: border-box;
  position: relative;
}
.defualt-inner-title-view {
  display: flex;
  height: 40rpx;
  line-height: 40rpx;
  width: 100%;
}
.abs-tel-img-view {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 26rpx;
  top: 20rpx;
}
.default-tag-color-view {
  width: 6rpx;
  height: 30rpx;
  background-color: #FF6600;
  margin: 5rpx 20rpx 0px 0px;
}
.default-title-text-view {
  font-size: 28rpx;
  color: #333333;
  font-weight: 600;
}
.default-inner-info-label-view {
  width: 100%;
  font-size: 24rpx;
  color: #666666;
}
.flex-label-text-view {
  display: flex;
  flex-flow: wrap;
  line-height: 40rpx;
  width: 100%;
}
.more-label-text-view {
  width: 100%;
  line-height: 35rpx;
}
